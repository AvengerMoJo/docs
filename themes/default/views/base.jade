    .container
      .breadcrumb
          ul.page-list
            li
              a(href="http://auth0.com/docs") Documentation
            li &raquo;
            li=title

          ul.external-links
            li
              a(href="http://twitter.com/authzero", target="_blank") Twitter

            li
              a(href="http://github.com/auth0", target="_blank") Github

            li
              a(href="https://chat.auth0.com", target="_blank") Chat With Us

            li
